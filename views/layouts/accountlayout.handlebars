                <li><a href="account"><span class="link-title">E-mail notifications</span> <i class="mdi mdi-email-outline link-icon"></i></a></li>
                <li class="nav-category-divider" style="position: static">ACCOUNT</li>
                <li><a href="account/settings"><span class="link-title">Settings</span> <i class="mdi mdi-settings link-icon"></i></a></li>
                <li><a href="logout"><span class="link-title">Log out</span> <i class="mdi mdi-key-variant link-icon"></i></a></li>
                <li class="nav-category-divider" style="position: static; margin-top:10rem">FEEDBACK</li>
                <li><a href="account/contact"><span class="link-title">Contact</span> <i class="mdi mdi-flask-outline link-icon"></i></a></li>
            </ul>
            <div class="sidebar_footer">
                <div class="btn-group admin-access-level">
                    <div class="avatar"><img class="profile-img" src="../assets/images/microscope.png" alt=""></div>
                    <div class="user-type-wrapper">
                        <p class="user_name">{{{username}}}</p>
                        <div class="d-flex align-items-center">
                            <div class="status-indicator small rounded-indicator bg-success"></div><small class="user_access_level">User</small></div>
                    </div><!--<i class="arrow mdi mdi-chevron-right"></i>--></div>
            </div>
        </div>
